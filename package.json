  "scripts": {
    "test": "clear && mocha --compilers js:babel-core/register --require ./tests/test_setup.jsx --recursive \"tests/**/*.@(js|jsx)\"",
    "test:watch": "nodemon --exec 'npm run test --R' -e jsx,js,jade,graphql,ts,json",
