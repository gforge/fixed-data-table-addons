    "react",
    "table",
    "data"
  ],
  "author": "Max Gordon",
  "license": "MIT",
  "peerDependencies": {
    "react": "^0.14.x || ^15.x",
    "react-bootstrap": "^0.31.x",
    "react-dom": "^0.14.0 || ^15.x"
  },
