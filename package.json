{
  "name": "fixed-data-table-addons",
  "version": "0.3.2",
  "description": "Addons for the fixed-data-table & fixed-data-table-2",
  "main": "build/index.js",
  "scripts": {
    "test": "clear && mocha --require babel-register --require ./tests/test_setup.jsx --recursive \"tests/**/*.@(js|jsx)\"",
    "test:watch": "nodemon --exec 'npm run test --R' -e jsx,js,jade,graphql,ts,json",
    "cover": "clear && istanbul cover _mocha -- --require babel-core/register --require ./tests/test_setup.jsx --recursive \"tests/**/*.@(js|jsx)\"",
    "build": "babel src --out-dir build",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gforge/fixed-data-table-addons.git"
  },
  "keywords": [
    "fixed-data-table",
    "react",
    "table",
    "data"
  ],
  "author": "Max Gordon",
  "license": "MIT",
  "peerDependencies": {
    "react": "^0.14.x || ^15.x",
    "react-bootstrap": "^0.31.x",
    "react-dom": "^0.14.0 || ^15.x"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-require": "^1.0.1",
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.0",
    "cheerio": "^1.0.0-rc.2",
    "dirty-chai": "^2.0.1",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.4.0",
    "fixed-data-table-2": "^0.8.1",
    "flow-bin": "^0.56.0",
    "istanbul": "^0.4.5",
    "jsdom": "^11.3.0",
    "mocha": "^4.0.0",
    "nodemon": "^1.12.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "sinon": "^4.0.1"
  },
  "bugs": {
    "url": "https://github.com/gforge/fixed-data-table-addons/issues"
  },
  "homepage": "https://github.com/gforge/fixed-data-table-addons#readme",
  "dependencies": {
    "except": "^0.1.3",
    "prop-types": "^15.6.0"
  }
